{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "indent": [
      "warn",
      2
    ],
    "treatUndefinedAsUnspecified": true,
    "func-names": "off",
    "consistent-return": "off",
    "no-unused-labels": "warn",
    "no-underscore-dangle": "error"
  }
}